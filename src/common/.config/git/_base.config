[core]
	ignorecase = false
	autocrlf = false
	quotepath = false

[color]
	ui = true

[push]
	default = current

[pull]
 	rebase = true
	ff = only

[init]
	defaultBranch = main

[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true

[ghq]
	root = ~/Repositories